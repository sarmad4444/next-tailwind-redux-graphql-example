{
  "name": "server",
  "version": "1.0.0",
  "description": "The Backend API for oCombat",
  "main": "server.js",
  "repository": "https://github.com/ocombat/ocombat-app",
  "author": "OCombat",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "babel src --out-dir dist",
    "start": "node dist/index.js",
    "dev": "nodemon --exec npx babel-node src/server.js",
    "prettier": "prettier --config ./.prettierrc --write \"**/*.js\"",
    "pretest": "eslint --ignore-path .gitignore .",
    "postinstall": "rm -rf dist && yarn run build",
    "lint": "yarn prettier --write --check --config ./.prettierrc \"**/*.js\" && eslint --fix ./src",
    "release": "release-it patch --no-npm.publish"
  },
  "dependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.15.0",
    "apollo-engine": "^1.1.2",
    "apollo-server-express": "^3.1.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-compose": "^9.0.2",
    "graphql-compose-connection": "^8.2.1",
    "graphql-compose-mongoose": "^9.5.2",
    "graphql-middleware": "^6.1.2",
    "graphql-tools": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.7",
    "mongoose-bcrypt": "^1.9.0",
    "mongoose-timestamp": "^0.6.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-throw-expressions": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "fs-extra": "^10.0.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2"
  }
}
